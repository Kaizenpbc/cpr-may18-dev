{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:test": "vite --config vite.config.test.ts --port 5174 --host",
    "build": "tsc -b && vite build",
    "build:test": "NODE_ENV=test tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "test:components": "vitest run src/components/**/*.test.{ts,tsx}",
    "test:services": "vitest run src/services/**/*.test.{ts,tsx}",
    "test:contexts": "vitest run src/contexts/**/*.test.{ts,tsx}",
    "test:portals": "vitest run src/components/portals/**/*.test.{ts,tsx}"
  },
  "dependencies": {
    "@date-io/date-fns": "^2.17.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^5.15.6",
    "@mui/material": "^5.15.6",
    "@mui/x-date-pickers": "^6.20.2",
    "@tanstack/react-query": "^5.80.7",
    "@types/react-toastify": "^4.0.2",
    "axios": "^1.6.7",
    "date-fns": "^2.30.0",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-phone-number-input": "^3.4.12",
    "react-router-dom": "^6.30.1",
    "react-toastify": "^11.0.5",
    "recharts": "^2.15.4",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "jsdom": "^24.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.3.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.4",
    "zod": "^3.25.28"
  }
