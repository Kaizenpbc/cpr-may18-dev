{
  "name": "backend",
  "version": "1.0.0",
  "description": "CPR Training System Backend",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "dev:test": "tsx watch src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "restart": "node scripts/restart-server.js",
    "migrate": "knex migrate:latest",
    "migrate:test": "NODE_ENV=test DB_NAME_TEST=cpr_jun21_test knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:status": "knex migrate:status",
    "seed": "knex seed:run",
    "seed:test": "NODE_ENV=test DB_NAME_TEST=cpr_jun21_test knex seed:run",
    "seed:test-data": "node scripts/seed-test-data.cjs",
    "init:test-migrations": "node scripts/init-test-migrations.cjs",
    "sync:test": "node scripts/sync-test-migrations.cjs",
    "sync:test-full": "npm run migrate:test && npm run seed:test-data",
    "seed:make": "knex seed:make",
    "db:init": "ts-node -e \"import('./src/config/database.js').then(db => db.initializeDatabase())\"",
    "db:reset": "ts-node scripts/reset-database.ts",
    "db:status": "ts-node scripts/check-db.ts",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "db:setup": "npm run db:migrate && npm run db:seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^9.0.2",
    "@lifeomic/attempt": "^3.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express-session": "^1.18.1",
    "@types/knex": "^0.15.2",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/socket.io": "^3.0.1",
    "@types/validator": "^13.15.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "connect-redis": "^8.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "html-pdf-node": "^1.0.7",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "node-cron": "^4.0.7",
    "nodemailer": "^7.0.3",
    "pg": "^8.16.0",
    "prom-client": "^15.1.0",
    "puppeteer": "^24.9.0",
    "redis": "^5.5.5",
    "socket.io": "^4.8.1",
    "validator": "^13.15.15",
    "vite": "6.3.5",
    "winston": "^3.17.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.0",
    "chalk": "^5.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.3.3"
  }
}
